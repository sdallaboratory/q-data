{
  "name": "orchestrator",
  "version": "1.0.0",
  "description": "Accepts tasks and controls fulfillment",
  "main": "main.ts",
  "scripts": {
    "start": "nodemon --watch . --watch ../shared --ignore ./dist --ext json,ts,js --exec npm run serve:dev",
    "serve:dev": "npm run build && npm run serve",
    "serve": "node ./dist/orchestrator/src/main.js",
    "build": "tsc",
    "build:watch": "nodemon --watch . --watch ../shared --ignore ./dist --ext json,ts,js --exec npm run build"
  },
  "author": "Sergei Solovev",
  "license": "ISC",
  "dependencies": {
    "bullmq": "^1.81.1",
    "express": "^4.18.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "nodemon": "^2.0.16",
    "typescript": "^4.6.4"
  }
}
